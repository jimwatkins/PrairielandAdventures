<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools" android:layout_width="match_parent"
    android:layout_height="match_parent" android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    android:paddingBottom="@dimen/activity_vertical_margin"
    tools:context="com.bigskyway.skyler.prairielandadventures2.FirstLevel"
    android:id="@+id/RelativeLayout">

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:text="Text"
        android:id="@+id/txtEnglishAnswer"
        android:layout_alignParentTop="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:textColor="#010101" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Snake Health"
        android:id="@+id/txtSnakeHealth"
        android:layout_alignParentRight="true" />

    <FrameLayout
        android:layout_width="fill_parent"
        android:layout_height="fill_parent"
        android:layout_below="@+id/txtEnglishAnswer"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true"
        android:id="@+id/FrameLayout">

        <FrameLayout
            android:layout_width="96dp"
            android:layout_height="138dp"
            android:layout_gravity="left|center_vertical"
            android:id="@+id/flaySnakeWord1">

            <ImageButton
                android:layout_width="74dp"
                android:layout_height="82dp"
                android:layout_weight=".33"
                android:background="@drawable/startersnaketwo"
                android:id="@+id/imgSnake1"
                android:layout_gravity="top|center_horizontal" />

            <TextView
                android:layout_width="match_parent"
                android:layout_height="139dp"
                android:textAppearance="?android:attr/textAppearanceSmall"
                android:text="Text"
                android:id="@+id/txtVSpan1"
                android:textSize="16dp"
                android:textStyle="bold"
                android:layout_below="@+id/txtEnglishAnswer"
                android:layout_alignRight="@+id/txtEnglishAnswer"
                android:layout_alignEnd="@+id/txtEnglishAnswer"
                android:layout_alignBottom="@+id/textView3"
                android:layout_gravity="left|center_vertical|bottom"
                android:gravity="bottom|center_horizontal"
                android:textColor="#010101" />
        </FrameLayout>

        <FrameLayout
            android:layout_width="102dp"
            android:layout_height="149dp"
            android:layout_gravity="center_horizontal|top"
            android:id="@+id/flaySnakeWord2">

            <ImageButton
                android:layout_width="59dp"
                android:layout_height="95dp"
                android:id="@+id/imgSnake2"
                android:layout_weight=".33"
                android:background="@drawable/startersnaketwo"
                android:layout_gravity="center_horizontal|top" />

            <TextView
                android:layout_width="match_parent"
                android:layout_height="150dp"
                android:textAppearance="?android:attr/textAppearanceSmall"
                android:text="Text"
                android:id="@+id/txtVSpan2"
                android:layout_centerHorizontal="true"
                android:textStyle="bold"
                android:textSize="16dp"
                android:layout_below="@+id/txtEnglishAnswer"
                android:layout_gravity="bottom|center_horizontal"
                android:gravity="bottom|center_horizontal"
                android:textColor="#010101" />
        </FrameLayout>

        <FrameLayout
            android:layout_width="91dp"
            android:layout_height="152dp"
            android:layout_gravity="right|center_vertical"
            android:id="@+id/flaySnakeWord3">

            <ImageButton
                android:layout_width="74dp"
                android:layout_height="91dp"
                android:id="@+id/imgSnake3"
                android:layout_weight=".33"
                android:background="@drawable/startersnaketwo"
                android:focusableInTouchMode="false"
                android:layout_gravity="top|center_horizontal" />

            <TextView
                android:layout_width="103dp"
                android:layout_height="149dp"
                android:textAppearance="?android:attr/textAppearanceSmall"
                android:text="Text"
                android:id="@+id/txtVSpan3"
                android:layout_centerHorizontal="false"
                android:textStyle="bold"
                android:textSize="16dp"
                android:layout_below="@+id/txtEnglishAnswer"
                android:layout_gravity="bottom|center_horizontal"
                android:gravity="bottom|center_horizontal"
                android:textColor="#010101" />
        </FrameLayout>

        <FrameLayout
            android:layout_width="124dp"
            android:layout_height="152dp"
            android:layout_gravity="center_horizontal|bottom"
            android:id="@+id/flayHeroTimer">

            <ImageView
                android:layout_width="80dp"
                android:layout_height="80dp"
                android:id="@+id/ivHero"
                android:layout_gravity="center"
                android:background="@drawable/ggggbehindview" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textAppearance="?android:attr/textAppearanceLarge"
                android:text="5"
                android:id="@+id/txtTimer"
                android:layout_gravity="top|center"
                android:textColor="#010101" />

            <TextView
                android:layout_width="108dp"
                android:layout_height="29dp"
                android:textAppearance="?android:attr/textAppearanceSmall"
                android:text="Health"
                android:id="@+id/TxtHealth"
                android:layout_gravity="bottom|center_horizontal"
                android:layout_marginTop="30dp"
                android:textColor="#010101"
                android:gravity="bottom|center_horizontal" />
        </FrameLayout>

    </FrameLayout>

</RelativeLayout>
