<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools" android:layout_width="match_parent"
    android:layout_height="match_parent" android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    android:paddingBottom="@dimen/activity_vertical_margin"
    tools:context="com.bigskyway.skyler.prairielandadventures2.FirstLevel"
    android:id="@+id/RelativeLayout">

    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/ivMaze"
        android:layout_alignParentTop="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:src="@drawable/challenge2_maze"
        android:layout_alignParentBottom="true"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true" />

    <FrameLayout
        android:layout_width="fill_parent"
        android:layout_height="fill_parent"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true"
        android:id="@+id/FrameLayout">

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textAppearance="?android:attr/textAppearanceLarge"
            android:text="5"
            android:id="@+id/txtTimer"
            android:layout_gravity="bottom|center|right"
            android:textColor="#010101" />

        <FrameLayout
            android:layout_width="128dp"
            android:layout_height="149dp"
            android:layout_gravity="center_horizontal|top"
            android:id="@+id/flaySnakeWord2">

            <ImageButton
                android:layout_width="59dp"
                android:layout_height="wrap_content"
                android:id="@+id/imgSnake2"
                android:layout_weight=".33"
                android:background="@drawable/startersnaketwo"
                android:layout_gravity="center_horizontal|top" />

            <TextView
                android:layout_width="146dp"
                android:layout_height="95dp"
                android:textAppearance="?android:attr/textAppearanceSmall"
                android:text="Text"
                android:id="@+id/txtVSpan2"
                android:layout_centerHorizontal="true"
                android:textStyle="bold"
                android:textSize="18dp"
                android:layout_below="@+id/txtEnglishAnswer"
                android:layout_gravity="top|center_horizontal"
                android:gravity="bottom|center_horizontal"
                android:textColor="#010101" />
        </FrameLayout>

        <FrameLayout
            android:layout_width="113dp"
            android:layout_height="152dp"
            android:layout_gravity="right|center_vertical"
            android:id="@+id/flaySnakeWord3">

            <ImageButton
                android:layout_width="87dp"
                android:layout_height="wrap_content"
                android:id="@+id/imgSnake3"
                android:layout_weight=".33"
                android:background="@drawable/attackingsnakeposthreetwo"
                android:focusableInTouchMode="false"
                android:layout_gravity="top|right|center_horizontal" />

            <TextView
                android:layout_width="103dp"
                android:layout_height="77dp"
                android:textAppearance="?android:attr/textAppearanceSmall"
                android:text="Text"
                android:id="@+id/txtVSpan3"
                android:layout_centerHorizontal="false"
                android:textStyle="bold"
                android:textSize="18dp"
                android:layout_below="@+id/txtEnglishAnswer"
                android:layout_gravity="top|center_horizontal"
                android:gravity="bottom|center_horizontal"
                android:textColor="#010101" />
        </FrameLayout>

        <ImageView
            android:layout_width="71dp"
            android:layout_height="71dp"
            android:id="@+id/ivHero"
            android:layout_gravity="left|bottom"
            android:background="@drawable/ggggbehindview" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textAppearance="?android:attr/textAppearanceLarge"
            android:text="Text"
            android:id="@+id/textView"
            android:layout_alignParentTop="true"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true"
            android:textColor="#010101"
            android:layout_gravity="bottom|center_horizontal" />

        <FrameLayout
            android:layout_width="118dp"
            android:layout_height="138dp"
            android:layout_gravity="left|top"
            android:id="@+id/flaySnakeWord1">

            <ImageButton
                android:layout_width="wrap_content"
                android:layout_height="111dp"
                android:layout_weight=".33"
                android:background="@drawable/attackingsnakeposthreetwo"
                android:id="@+id/imgSnake1"
                android:layout_gravity="top|left|center_vertical" />

            <TextView
                android:layout_width="126dp"
                android:layout_height="84dp"
                android:textAppearance="?android:attr/textAppearanceSmall"
                android:text="Text"
                android:id="@+id/txtVSpan1"
                android:textSize="18dp"
                android:textStyle="bold"
                android:layout_below="@+id/txtEnglishAnswer"
                android:layout_alignRight="@+id/txtEnglishAnswer"
                android:layout_alignEnd="@+id/txtEnglishAnswer"
                android:layout_alignBottom="@+id/textView3"
                android:layout_gravity="top|left|center_vertical"
                android:gravity="bottom|center_horizontal"
                android:textColor="#010101" />
        </FrameLayout>

        <FrameLayout
            android:layout_width="113dp"
            android:layout_height="152dp"
            android:layout_gravity="center"
            android:id="@+id/flaySnakeWord4" >

            <ImageButton
                android:layout_width="wrap_content"
                android:layout_height="123dp"
                android:id="@+id/imgSnake4"
                android:layout_weight=".33"
                android:background="@drawable/attackingsnakeposthreetwo"
                android:focusableInTouchMode="false"
                android:layout_gravity="top|right|center_horizontal" />

            <TextView
                android:layout_width="103dp"
                android:layout_height="77dp"
                android:textAppearance="?android:attr/textAppearanceSmall"
                android:text="Text"
                android:id="@+id/txtVSpan4"
                android:layout_centerHorizontal="false"
                android:textStyle="bold"
                android:textSize="18dp"
                android:layout_below="@+id/txtEnglishAnswer"
                android:layout_gravity="top|center_horizontal"
                android:gravity="bottom|center_horizontal"
                android:textColor="#010101" />
        </FrameLayout>

        <FrameLayout
            android:layout_width="118dp"
            android:layout_height="138dp"
            android:layout_gravity="left|center_vertical"
            android:id="@+id/flaySnakeWord5" >

            <ImageButton
                android:layout_width="wrap_content"
                android:layout_height="111dp"
                android:layout_weight=".33"
                android:background="@drawable/attackingsnakeposthreetwo"
                android:id="@+id/imgSnake5"
                android:layout_gravity="top|left|center_vertical" />

            <TextView
                android:layout_width="126dp"
                android:layout_height="84dp"
                android:textAppearance="?android:attr/textAppearanceSmall"
                android:text="Text"
                android:id="@+id/txtVSpan5"
                android:textSize="18dp"
                android:textStyle="bold"
                android:layout_below="@+id/txtEnglishAnswer"
                android:layout_alignRight="@+id/txtEnglishAnswer"
                android:layout_alignEnd="@+id/txtEnglishAnswer"
                android:layout_alignBottom="@+id/textView3"
                android:layout_gravity="top|left|center_vertical"
                android:gravity="bottom|center_horizontal"
                android:textColor="#010101" />
        </FrameLayout>

        <FrameLayout
            android:layout_width="118dp"
            android:layout_height="138dp"
            android:layout_gravity="center_horizontal|bottom"
            android:id="@+id/flaySnakeWord6" >

            <ImageButton
                android:layout_width="wrap_content"
                android:layout_height="111dp"
                android:layout_weight=".33"
                android:background="@drawable/attackingsnakeposthreetwo"
                android:id="@+id/imgSnake6"
                android:layout_gravity="top|left|center_vertical" />

            <TextView
                android:layout_width="126dp"
                android:layout_height="84dp"
                android:textAppearance="?android:attr/textAppearanceSmall"
                android:text="Text"
                android:id="@+id/txtVSpan6"
                android:textSize="18dp"
                android:textStyle="bold"
                android:layout_below="@+id/txtEnglishAnswer"
                android:layout_alignRight="@+id/txtEnglishAnswer"
                android:layout_alignEnd="@+id/txtEnglishAnswer"
                android:layout_alignBottom="@+id/textView3"
                android:layout_gravity="top|left|center_vertical"
                android:gravity="bottom|center_horizontal"
                android:textColor="#010101" />
        </FrameLayout>

        <FrameLayout
            android:layout_width="118dp"
            android:layout_height="138dp"
            android:layout_gravity="right|bottom"
            android:id="@+id/flaySnakeWord7" >

            <ImageButton
                android:layout_width="wrap_content"
                android:layout_height="111dp"
                android:layout_weight=".33"
                android:background="@drawable/attackingsnakeposthreetwo"
                android:id="@+id/imgSnake7"
                android:layout_gravity="top|left|center_vertical" />

            <TextView
                android:layout_width="126dp"
                android:layout_height="84dp"
                android:textAppearance="?android:attr/textAppearanceSmall"
                android:text="Text"
                android:id="@+id/txtVSpan7"
                android:textSize="18dp"
                android:textStyle="bold"
                android:layout_below="@+id/txtEnglishAnswer"
                android:layout_alignRight="@+id/txtEnglishAnswer"
                android:layout_alignEnd="@+id/txtEnglishAnswer"
                android:layout_alignBottom="@+id/textView3"
                android:layout_gravity="top|left|center_vertical"
                android:gravity="bottom|center_horizontal"
                android:textColor="#010101" />
        </FrameLayout>

        <FrameLayout
            android:layout_width="118dp"
            android:layout_height="138dp"
            android:layout_gravity="right|top"
            android:id="@+id/flayHealth" >

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="27dp"
                android:textAppearance="?android:attr/textAppearanceSmall"
                android:text="Health"
                android:id="@+id/textView4"
                android:layout_gravity="top|center_horizontal|right"
                android:layout_marginTop="30dp"
                android:textColor="#010101" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="73dp"
                android:text="Snake Health"
                android:id="@+id/textView5"
                android:layout_alignParentRight="true"
                android:textColor="#fe0101"
                android:layout_gravity="bottom|right" />
        </FrameLayout>

    </FrameLayout>

</RelativeLayout>
