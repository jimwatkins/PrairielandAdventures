<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools" android:layout_width="match_parent"
    android:layout_height="match_parent" android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    android:paddingBottom="@dimen/activity_vertical_margin"
    tools:context="com.bigskyway.skyler.prairielandadventures2.FirstLevel"
    android:id="@+id/RelativeLayout">

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:text="Text"
        android:id="@+id/txtEnglishAnswer"
        android:layout_alignParentTop="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Snake Health"
        android:id="@+id/txtSnakeHealth"
        android:layout_alignParentRight="true" />

    <FrameLayout
        android:layout_width="fill_parent"
        android:layout_height="fill_parent"
        android:layout_below="@+id/txtEnglishAnswer"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true"
        android:id="@+id/FrameLayout">

        <ImageView
            android:layout_width="124dp"
            android:layout_height="124dp"
            android:id="@+id/Gary"
            android:layout_gravity="center_horizontal|bottom"
            android:background="@drawable/ggggbehindview" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textAppearance="?android:attr/textAppearanceSmall"
            android:text="Health"
            android:id="@+id/TxtHealth"
            android:layout_gravity="left|bottom"
            android:layout_marginTop="30dp" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textAppearance="?android:attr/textAppearanceLarge"
            android:text="5"
            android:id="@+id/txtTimer"
            android:layout_gravity="center" />

        <ImageButton
            android:layout_width="wrap_content"
            android:layout_height="130dp"
            android:id="@+id/imgSnake3"
            android:layout_weight=".33"
            android:background="@drawable/startersnaketwo"
            android:focusableInTouchMode="false"
            android:layout_gravity="right|center_vertical" />

        <TextView
            android:layout_width="85dp"
            android:layout_height="162dp"
            android:textAppearance="?android:attr/textAppearanceSmall"
            android:text="Text"
            android:id="@+id/txtVSpan2"
            android:layout_centerHorizontal="true"
            android:textStyle="bold"
            android:textSize="20dp"
            android:layout_below="@+id/txtEnglishAnswer"
            android:layout_gravity="center_horizontal|top" />

        <TextView
            android:layout_width="102dp"
            android:layout_height="40dp"
            android:textAppearance="?android:attr/textAppearanceSmall"
            android:text="Text"
            android:id="@+id/txtVSpan1"
            android:textSize="20dp"
            android:textStyle="bold"
            android:layout_below="@+id/txtEnglishAnswer"
            android:layout_alignRight="@+id/txtEnglishAnswer"
            android:layout_alignEnd="@+id/txtEnglishAnswer"
            android:layout_alignBottom="@+id/textView3"
            android:layout_gravity="left|center_vertical" />

        <ImageButton
            android:layout_width="74dp"
            android:layout_height="119dp"
            android:id="@+id/imgSnake2"
            android:layout_weight=".33"
            android:background="@drawable/startersnaketwo"
            android:layout_gravity="center_horizontal|top" />

        <ImageButton
            android:layout_width="wrap_content"
            android:layout_height="111dp"
            android:layout_weight=".33"
            android:background="@drawable/startersnaketwo"
            android:id="@+id/imgSnake1"
            android:layout_gravity="left|center_vertical" />

        <TextView
            android:layout_width="103dp"
            android:layout_height="111dp"
            android:textAppearance="?android:attr/textAppearanceSmall"
            android:text="Text"
            android:id="@+id/txtVSpan3"
            android:layout_centerHorizontal="false"
            android:gravity="right"
            android:textStyle="bold"
            android:textSize="20dp"
            android:layout_below="@+id/txtEnglishAnswer"
            android:layout_gravity="right|center_vertical" />

    </FrameLayout>

</RelativeLayout>
